<h1><u>Adnat</u></h1>

  <h2>Login</h2>
  <%= form_with(url: login_path, scope: :session, local: true) do |f| %>

    <%= f.label :email %><br>
    <%= f.email_field :email %><br><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>

    <%= f.submit "Log in", class: "btn btn-primary" %>
  <% end %>

  <p>New user? <%= link_to "Sign up now!", signup_path %></p>